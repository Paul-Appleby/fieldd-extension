{
  "version": 2,
  "builds": [
    {
      "src": "api/route.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/map-embed.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/places.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/route",
      "dest": "/api/route.js"
    },
    {
      "src": "/api/map-embed",
      "dest": "/api/map-embed.js"
    },
    {
      "src": "/api/places",
      "dest": "/api/places.js"
    }
  ]
} 